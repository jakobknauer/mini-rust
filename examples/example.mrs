fn main() -> i32 {
    fac(5)
}

fn fac(n: i32) -> i32 {
    let result = 1;
    loop {
        if n == 0 {
            return result;
        }
        result = result * n;
        n = n - 1
    }
    result
}

fn scalar_product(p: Point, q: Point) -> i32 {
    q.x = 5;
    p.x * q.x + p.y * q.y
}

struct Point {
    x: i32,
    y: i32,
}
